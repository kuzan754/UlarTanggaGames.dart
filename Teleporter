/**
 *
 * @author i14007
 */
public abstract class Teleporter {
    private int identifier;
    private int from;
    private int to;
    
    public Teleporter(int from,int to,int identifier){
        this.identifier = identifier;
        this.from = from;
        this.to = to;
    }

    public int getFrom() {
        return from;
    }

    public int getTo() {
        return to;
    }
    
    public int getIdentifier(){
        return identifier;
    }
}
