/**
 *
 * @author i14007
 */
public class ComputerPlayer extends Player{
    public ComputerPlayer(String name,char symbol) {
        super(name,symbol);
    }
    
    @Override
    public void move(int input){
        this.setCurPosition(this.getCurPosition()+input);
    }
}
